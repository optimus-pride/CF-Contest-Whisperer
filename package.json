{
	"name": "cf-contest-whisperer",
	"description": "Submition notifications in vscode",
	"version": "0.0.1",
	"publisher": "Amitabh Singh",
	"icon": "icon.png",
	"displayName": "CF contest whisperer",
	"repository": "https://github.com/optimus-pride/CF-Contest-Whisperer",
	"engines": {
		"vscode": "^1.74.0"
	},
	"activationEvents": [
		"onStartupFinished"
	],
	"main": "./extension.js",
	"contributes": {
		"configuration": {
			"title": "cf contest whisperer",
			"properties": {
				"codeforces-notify.handle": {
					"type": "string",
					"default": "",
					"description": "Codeforces user handle"
				}
			}
		}
	},
	"scripts": {},
	"devDependencies": {
		"@types/vscode": "^1.73.0"
	},
	"dependencies": {
		"axios": "^1.7.2"
	}
}
